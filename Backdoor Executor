local PilinExecutor = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local ExecuteButton = Instance.new("TextButton")
local ClearButton = Instance.new("TextButton")
local InjectButton = Instance.new("TextButton")
local ScriptBox = Instance.new("TextBox")
local UICorner = Instance.new("UICorner")
local UIStroke = Instance.new("UIStroke")
local DecorationImage = Instance.new("ImageLabel")

PilinExecutor.Name = "PilinExecutor"
PilinExecutor.ResetOnSpawn = false
PilinExecutor.Parent = game.CoreGui

local TRUE_BLACK = Color3.fromRGB(0, 0, 0)
local RICH_ORANGE = Color3.fromRGB(255, 85, 0)

MainFrame.Name = "MainFrame"
MainFrame.Parent = PilinExecutor
MainFrame.BackgroundColor3 = TRUE_BLACK
MainFrame.Size = UDim2.new(0, 500, 0, 250)
MainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
MainFrame.Active = true
MainFrame.Selectable = true

UIStroke.Parent = MainFrame
UIStroke.Color = RICH_ORANGE
UIStroke.Thickness = 2.5

UICorner.Parent = MainFrame
UICorner.CornerRadius = UDim.new(0, 8)

DecorationImage.Name = "DecorationImage"
DecorationImage.Parent = MainFrame
DecorationImage.BackgroundTransparency = 1
DecorationImage.Size = UDim2.new(0, 32, 0, 32)
DecorationImage.Position = UDim2.new(0.01, 5, 0.02, 0)
DecorationImage.Image = "rbxassetid://17812418693"
DecorationImage.ScaleType = Enum.ScaleType.Fit
DecorationImage.ZIndex = 2

local titleText = "PILIN EXECUTOR v3.0"
Title.Name = "Title"
Title.Parent = MainFrame
Title.Text = ""
Title.Size = UDim2.new(0.75, 0, 0.15, 0)
Title.Position = UDim2.new(0.12, 0, 0.02, 0)
Title.TextSize = 18
Title.TextColor3 = RICH_ORANGE
Title.BackgroundTransparency = 1
Title.Font = Enum.Font.GothamBlack
Title.TextStrokeTransparency = 0.8
Title.TextXAlignment = Enum.TextXAlignment.Left

spawn(function()
    for i = 1, #titleText do
        Title.Text = string.sub(titleText, 1, i)
        wait(0.08)
    end
end)

ScriptBox.Name = "ScriptBox"
ScriptBox.Parent = MainFrame
ScriptBox.Size = UDim2.new(0.9, 0, 0.5, 0)
ScriptBox.Position = UDim2.new(0.05, 0, 0.2, 0)
ScriptBox.MultiLine = true
ScriptBox.BackgroundColor3 = TRUE_BLACK
ScriptBox.TextColor3 = Color3.fromRGB(255, 255, 255)
ScriptBox.Text = ""
ScriptBox.ClearTextOnFocus = false
ScriptBox.Font = Enum.Font.RobotoMono
